function request(paras) {
    var url = location.href;
    var paraString = url.substring(url.indexOf("?") + 1, url.length).split("&");
    var paraObj = {}
    for (i = 0; j = paraString[i]; i++) {
        paraObj[j.substring(0, j.indexOf("=")).toLowerCase()] = j.substring(j.indexOf("=") + 1, j.length);
    }
    var returnValue = paraObj[paras.toLowerCase()];
    if (typeof (returnValue) == "undefined") {
        return "";
    } else {
        return returnValue;
    }
}

function countSubStr(mainStr, subStr) {
    var count = 0;
    var offset = 0;
    do {
        offset = mainStr.indexOf(subStr, offset);
        if (offset != -1) {
            count++;
            offset += subStr.length;
        }
    } while (offset != -1)
    return count;
}

var complete = 'TEST COMPLETE';
var pass = 'PASS'
function compareExpectedActual(expect, actual) {
    if (actual.length <= 0)
        return false;
    if (expect.toLowerCase() == actual.toLowerCase()) {
        return true;
    }
    var ec = countSubStr(expect, complete);
    var ac = countSubStr(actual, complete);
    var ep = countSubStr(expect, pass);
    var ap = countSubStr(actual, pass);
    //alert(expect);
    //alert(ac + '||' + ap + ' vs ' + ec + '||' + ep);
    if (ac + ap > 0 && ac == ec && ap == ep) {
       // alert('ac+ap == ec+ep');

        return true;
    }
    return false;

}